{
  "name": "bookmark-manager",
  "version": "9.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/webpack.config.js --env env=dev  --progress",
    "build": "webpack            --config config/webpack/webpack.config.js --env env=prod --progress --watch",
    "start-server": "tsx ./persistent-storage/server.ts",
    "clean": "rm -rf ./dist",
    "ts": "tsc",
    "ts-init": "tsc --init",
    "ts-watch": "clear && npm run clean && tsc --watch",
    "prettier": "prettier --write \"./**/*.{html,json,js,ts,jsx,tsx}\"",
    "license-summary": "license-checker --summary",
    "license-detail": "license-checker --out ./build/license-detail.txt",
    "license-ltd-types": "license-checker --out ./build/license-detail.txt --exclude [MIT,Apache-2.0,ISC,BSD-2-Clause,BSD-3-Clause]",
    "cloc": "cloc src && cloc persistent-storage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chrome": "0.0.251",
    "@types/cors": "2.8.17",
    "@types/node": "20.9.1",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "cloc": "2.11.0",
    "cors": "2.8.5",
    "css-loader": "6.10.0",
    "express": "4.19.2",
    "html-webpack-plugin": "5.5.3",
    "license-checker": "25.0.1",
    "prettier": "3.1.0",
    "sass": "1.70.0",
    "sass-loader": "14.1.0",
    "style-loader": "3.3.4",
    "ts-loader": "9.5.1",
    "tsx": "4.13.1",
    "typescript": "5.2.2",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.10.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "react-redux": "9.1.2",
    "redux": "5.0.1"
  }
}
